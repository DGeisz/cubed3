{
  "name": "cubed-server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@ethersproject/sha2": "^5.5.0",
    "@project-serum/anchor": "^0.20.1",
    "@solana/web3.js": "^1.32.0",
    "aws-sdk": "^2.1096.0",
    "canvas": "^2.9.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "mongoose": "^6.2.0",
    "three": "^0.138.3",
    "tweetnacl": "^1.0.3",
    "typescript": "^4.5.5",
    "underscore": "^1.13.2"
  },
  "scripts": {
    "build": "tsc",
    "prod": "tsc && node dist/js/index.js",
